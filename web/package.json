{
  "name": "arisan-web-app",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "concurrently \"next dev\" \"tailwindcss --input ./app/globals.css --output ./app/output.css --watch\"",
    "custom-install": "cd ../bnb-chain && npm install && cd ../web && npm install",
    "build": "cd ../bnb-chain && npx hardhat clean && npx hardhat compile && cd ../web && next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@enzoferey/ethers-error-parser": "^0.2.3",
    "@ethersproject/providers": "^5.7.2",
    "@headlessui/react": "^1.7.12",
    "@heroicons/react": "^2.0.16",
    "@reduxjs/toolkit": "^1.9.5",
    "@tremor/react": "^2.0.1",
    "@typechain/ethers-v5": "^10.2.0",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "18.14.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@walletconnect/web3-provider": "^1.8.0",
    "bufferutil": "^4.0.7",
    "encoding": "^0.1.13",
    "eslint": "8.35.0",
    "eslint-config-next": "13.2.1",
    "js-cookie": "^3.0.5",
    "kysely": "^0.23.4",
    "next": "^13.3.0",
    "next-auth": "^4.19.2",
    "prettier": "^2.8.4",
    "prop-types": "^15.8.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-redux": "^8.0.5",
    "react-spinner-overlay": "^0.1.33",
    "react-toastify": "^9.1.2",
    "redux": "^4.2.1",
    "redux-thunk": "^2.4.2",
    "server-only": "^0.0.1",
    "utf-8-validate": "^5.0.10",
    "web3modal": "^1.9.12"
  },
  "prettier": {
    "arrowParens": "always",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "none"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.0.1",
    "postcss": "^8.4.23",
    "tailwindcss": "^3.3.2",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4"
  }
